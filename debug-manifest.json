{
  "manifest_version": 3,
  "name": "文本翻译悬浮提示-调试版",
  "version": "1.0.0",
  "description": "鼠标悬浮元素时显示中文翻译 - 调试版本",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["debug-content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_title": "文本翻译悬浮提示-调试版"
  }
}
















































